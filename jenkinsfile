pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('BUILD') {
            steps {
                echo ' Dévéloppement'
            }
        }
        
        stage('Test') {
            steps {
                echo 'testing'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Mise en production'
            }
        }
    }
}
